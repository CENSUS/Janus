        "schm": "^0.4.1",
        "ssl-root-cas": "^1.3.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/node": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "cross-env": "^7.0.3",
