{
    "name": "moh-app-backend",
    "version": "1.0.0",
    "description": "-",
    "main": "./electron/electronInitiator.js",
    "build": {
        "appId": "com.ministry-of-health.app",
        "productName": "ministryOfHealth",
        "copyright": "Copyright Â© 2022",
        "extends": null
    },
    "scripts": {
        "remove-package-folder": "rm -rf dist/packages",
        "move-packages": "cp -r dist/packages/.[0-z]* dist/packages/* ../dist && rm -rf dist/packages/*",
        "move-build": "npm run remove-package-folder && cp -r dist/* ../build && rm -rf dist/* dist/.[0-z]* && rm -rf dist",
        "test": "echo \"Error: no test specified\" && exit 1",
        "electron-build-linux": "electron-builder -l",
        "electron-build-mac": "electron-builder -m",
        "electron-build-win": "electron-builder -w",
        "electron-dev": "cross-env ELECTRON_START_URL=http://localhost:3000 electron --inspect ./electron/electronInitiator.js"
    },
    "engineStrict": true,
    "author": "-",
    "license": "ISC",
    "dependencies": {
        "@fidm/x509": "^1.2.1",
        "@hyperledger/fabric-gateway": "^0.1.1-dev.20211110.10",
