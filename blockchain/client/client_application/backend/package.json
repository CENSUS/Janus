        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "electron-is-dev": "^2.0.0",
        "elliptic": "^6.5.4",
        "exectimer": "^2.2.2",
        "express": "^4.17.1",
        "express-validator": "^6.11.1",
        "fabric-ca-client": "^2.2.6",
        "fabric-common": "^2.2.9",
        "fabric-network": "^2.2.6",
        "fabric-protos": "^2.2.10",
